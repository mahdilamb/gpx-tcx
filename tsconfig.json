{
  "strict": true,
  "module": "esnext",
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./lib/",
    "lib": [
      "ESNext"
    ],
    "noImplicitAny": true,
    "strict": true,
    "module": "CommonJS",
    "target": "ESNext",
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules/*",
    "__sapper__/*",
  ],
}
